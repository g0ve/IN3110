"(\/\/.*)": comment
"(public\s|private\s)?(?:\w*)\s(?:\w*)\s(?:\w*)(?:\(.*\))(?!;)": method
"(public\s|private\s)?(?:class)\s(?:\w*)": class
"(\".*?\")": strings
"(import\s)(?:.*;)": import
"(\btrue|false\b)": statements
"(\bbreak|continue\b)(?:;)": break_continue
"(\bfor\b\s)(?:\((?:.*;.*;.*)\))": for_loops
"(\bwhile\b\s)(?:\(\w+\s*\)|\(\w+\s*(?:==|!=|<>|>|<|>=|<=)\s*\w+\))": while_loops
"(((if|else if)\s(?:\(\w+\s*\)|\(\w+\s*(?:==|!=|<>|>|<|>=|<=)\s*\w+\)))|else)": if_elif_else