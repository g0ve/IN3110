"(#.*)": comment
"(def\s)(?:.*:)": function
"(class\s)(?:\w*:)": class
"(([\'\"]{3}(?:\n|.)*?[\'\"]{3})|(\'.*?\'|\".*?\"))": strings
"((import\s)(?:\w*\s)|(from\s)(?:\w*))": import
"(\bNone|True|False\b)": statements
"(try|except)\s?(?:\w+\s*)?(?::)": try_except
"(\bfor\b\s)(?:\w*)(\sin\s)(?:range|\w+(?::))": for_loops
"(\bwhile\b)\s?(?:\w+\s*|\w+\s*(?:==|!=|<>|>|<|>=|<=)\s*\w+)?(?::)": while_loops
"(if|elif|else)\s?(?:\w+\s*|\w+\s*(?:==|!=|<>|>|<|>=|<=)\s*\w+)?(?::)": if_elif_else
