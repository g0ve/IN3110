"(#.*)": comment
"(def\s)": function
"(class\s)": class
"(([\"\']{3}.*?[\"\']{3})|(\'.*?\'|\".*?\"))": strings
"((import\s)|(from\s))": import
"(\bNone|True|False\b)": statements
"((try)|(except))": try_except
"(\bfor\b\s)(?:.*\w*)(\sin\s)": for_loops
"(\bwhile\b\s)": while_loops
"(\b(if|elif|else)\b\s)": if_elif_else
